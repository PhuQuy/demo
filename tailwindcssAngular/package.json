{
  "name": "tailwindcss-angular",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "set NODE_ENV=production && ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run tailwindcssAngular:serve-ssr",
    "serve:ssr": "node dist/tailwindcssAngular/server/main.js",
    "build:ssr": "npm run build:prod && ng run tailwindcssAngular:server",
    "build:docker": "docker build --no-cache -t phuquy/tailwind .",
    "prerender": "ng run tailwindcssAngular:prerender",
    "electron": "electron .",
    "serve:electron": "set NODE_ENV=production && ng build --configuration production --base-href ./ && electron .",
    "build:window": "set NODE_ENV=production && ng build --configuration production --base-href ./ && electron-packager . --platform=win32 --no-prune --ignore=/node_modules --ignore=/e2e --ignore=/src",
    "build:mac": "set NODE_ENV=production && ng build --configuration production --base-href ./ && electron-packager . --platform=darwin --no-prune --ignore=/node_modules --ignore=/e2e --ignore=/src"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.0.5",
    "@angular/common": "14.0.5",
    "@angular/compiler": "14.0.5",
    "@angular/core": "14.0.5",
    "@angular/forms": "14.0.5",
    "@angular/platform-browser": "14.0.5",
    "@angular/platform-browser-dynamic": "14.0.5",
    "@angular/platform-server": "14.0.5",
    "@angular/router": "14.0.5",
    "@angular/service-worker": "14.0.5",
    "@ngrx/store": "^14.0.2",
    "@nguniversal/express-engine": "^14.0.3",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.4",
    "compression": "^1.7.4",
    "express": "^4.15.2",
    "memory-cache": "^0.2.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.5",
    "@angular/cli": "~14.0.5",
    "@angular/compiler-cli": "14.0.5",
    "@nguniversal/builders": "^14.0.3",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.0.0",
    "@types/memory-cache": "^0.2.2",
    "@types/node": "^14.15.0",
    "autoprefixer": "^10.4.7",
    "electron": "^19.0.8",
    "electron-packager": "^15.5.1",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.6",
    "typescript": "~4.7.2"
  }
}